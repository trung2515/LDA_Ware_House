{
  "name": "mineral-inventory",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "proto:generate": "protoc --plugin=protoc-gen-ng=./node_modules/.bin/protoc-gen-ng --ng_out=worker=true:./src/app/core/models -I ./proto $(find ./proto -iname \"*.proto\")",
    "proto:generate:win": "for /f %G in ('dir /b .\\proto\\*.proto') do grpc_tools_node_protoc --plugin=protoc-gen-ng=.\\node_modules\\.bin\\protoc-gen-ng.cmd --ng_out=worker=true:.\\src\\app\\core\\models -I .\\proto .\\proto\\%G"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "devextreme": "^21.1.5",
    "devextreme-angular": "^21.1.5",
    "devextreme-schematics": "^1.2.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@ngx-grpc/common": "^1.0.4",
    "@ngx-grpc/core": "^1.0.4",
    "@ngx-grpc/grpc-web-client": "^1.0.4",
    "@ngx-grpc/protoc-gen-ng": "^1.0.4",
    "@ngx-grpc/worker": "^1.0.4",
    "@ngx-grpc/worker-client": "^1.0.4",
    "google-protobuf": "^3.15.5",
    "grpc-tools": "^1.11.2",
    "grpc-web": "^1.2.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "~12.2.9",
    "@angular/compiler-cli": "~12.2.0",
    "@types/google-protobuf": "^3.15.5",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "devextreme-cli": "latest",
    "devextreme-themebuilder": "^21.1.5",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
